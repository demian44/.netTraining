@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-dark" style="margin-top:4%">
    <thead>
        <tr>

            <th scope="col">@ViewBag.Message.title</th>
            <th scope="col">@ViewBag.Message.time</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Message.list)
        {
            <tr>
                <th scope="row">1</th>
                <td>@item</td>
                <td>Otto</td>
                <td>mdo</td>
            </tr>
        }


    </tbody>
</table>

